library(deepnet)
library(mxnet)
m = data.matrix(iris)[,1:4]
rbm = rbm.train(m, 2)

?mxnet::mx.model.FeedForward.create
?mxnet::mx.nd.array

mxW = mxnet::mx.nd.array(rbm$W)

ae = mx.symbol.Variable("data")
ae = mx.symbol.FullyConnected(ae, num_hidden = 4)
ae = mx.symbol.Activation(ae, act_type = "relu")
ae = mx.symbol.FullyConnected(ae, num_hidden = 2)
ae = mx.symbol.Activation(ae, act_type = "relu")
ae = mx.symbol.FullyConnected(ae, num_hidden = 4)
ae = mx.symbol.Activation(ae, act_type = "relu")
ae = mx.symbol.LinearRegressionOutput(ae)

mxnet::mx.model.FeedForward.create()
